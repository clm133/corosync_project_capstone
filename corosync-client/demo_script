1. start corosync / add node
2. print membership / quorum
3. monitor mode
4. node eligibility
4. stop connection / start connection
6. set votes

#node 1
./ctc -c start 12.0.0.1
./ctc -p membership
./ctc -c start 12.0.0.2
./ctc -p membership
./ctc p quorum
##move to node 2
./ctc -m quorum
##move back to node 1
./ctc -c start 12.0.0.3
./ctc -q set_votes 3 0
./ctc -q set_votes 3 1
./ctc -c start 12.0.0.4
#kill connection
./ctc -c kill_conn 12.0.0.3
./ctc -c kill_conn 12.0.0.4
#start connection
./ctc -c start_conn 12.0.0.4
./ctc -c start_conn 12.0.0.3

//talk about tie breaker
./ctc -c stop 12.0.0.1 12.0.0.2 12.0.0.3 12.0.0.4
./ctc -c reset_conf vm_tiebreaker.conf 12.0.0.1 12.0.0.2 12.0.0.3 12.0.0.4
./ctc -c start 12.0.0.1 12.0.0.2 12.0.0.3 12.0.0.4
#kill connection
./ctc -c kill_conn 12.0.0.3
./ctc -c kill_conn 12.0.0.4
